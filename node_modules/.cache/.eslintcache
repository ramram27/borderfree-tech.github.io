State } from \"react\";\nimport cart from \"./Images/assests/cart.jpg\";\nimport \"./Addcart.css\";\n\nconst AddCart = () => {\n\n  const [valueD, setvalue] =  useState(0);\n\n  const Additem = () => {\n      setvalue(valueD + 1);\n  }\n\n  const Removeitem = () => {\n    if(valueD !== 0) \n    setvalue(valueD - 1);\n  }\n\n  return ( \n    <>\n    <div className=\"headcart\">\n    <div className=\"value\">{valueD}</div>\n    <img src={cart} className=\"cart\" />\n    \n    </div>\n    \n    <div>\n      {\n        data.map((item) => {\n          return (\n            <>\n             <div className=\"pot\">\n             <img src={item.img} />\n             <div>{item.name}</div>\n             <div>Price Rs.{item.price}.00</div>\n              <div value={item.available} style={{color: \"red\",fontWeight : \"bold\"}}\n              > {item.available ? \"In-Stock\" : \"Out-of-Stock\"}</div>\n             <div>{item.vendor}</div>\n             <div>{item.category}</div>\n             {item.available ?\n             <>\n              <button onClick={Additem}> + </button>\n              <button onClick={Removeitem}> - </button>\n             </>\n              : \n              null\n              }\n             \n             </div> <br/><br/>\n            </>\n          )\n        })\n      }\n    </div>\n    \n    </>\n   );\n}\n \nexport default AddCart;","/home/raiadi2008/Desktop/React_Project/E-Commerce-shop-B/shoping-cart/src/index.jsx",[],"/home/raiadi2008/Desktop/React_Project/E-Commerce-shop-B/shoping-cart/src/App.jsx",[],"/home/raiadi2008/Desktop/React_Project/E-Commerce-shop-B/shoping-cart/src/Components/AddCart.jsx",["30","31"],{"ruleId":"32","severity":1,"message":"33","line":23,"column":5,"nodeType":"34","endLine":23,"endColumn":40},{"ruleId":"32","severity":1,"message":"33","line":33,"column":14,"nodeType":"34","endLine":33,"endColumn":36},{"ruleId":"32","severity":1,"message":"33","line":23,"column":5,"nodeType":"34","endLine":23,"endColumn":40},{"ru